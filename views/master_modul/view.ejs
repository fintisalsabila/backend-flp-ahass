<% layout('../partials/layout') -%>
<link rel="stylesheet" type="text/css" href="../css/pagination.css" />
<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha384-nvAa0+6Qg9clwYCGGPpDQLVpLNn0fRaROjHqs13t4Ggj3Ez50XnGQqc/r8MhnRDZ" crossorigin="anonymous"></script>

<div class="right_col" role="main">
    <div class="">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Master Modul<small>view</small></h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="form-horizontal form-label-left">
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Nama Modul:</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <p class="form-control-static"><%= data.modulName %></p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">File Upload:</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <p class="form-control-static"><a href="<%= data.fileUpload %>" target="_blank"><%= data.fileUpload %></a></p>
                                </div>
                            </div>

                            <div class="ln_solid"></div>
                            <div class="form-group">
                                <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                    <a href="/MasterModul/Index" class="btn btn-danger">Back</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- partial('../partials/footer') %>
</div>

<script src="../js/pagination.js"></script>
