<% layout('./partials/layout') -%>
    <script type="text/javascript">
        $().ready(function() {
            if (document.referrer != '<%= link %>') {
                history.pushState(null, null, '/');
                window.addEventListener('popstate', function() {
                    history.pushState(null, null, '/');
                });
            }
        });
    </script>
